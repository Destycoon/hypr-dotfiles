@theme "~/.config/rofi/theme.rasi"

configuration {
    show-icons: true;
    icon-theme: "Papirus-Dark"; 
    dpi: 96;


    modes: [ window , drun , run ];
   
    
    timeout {
        action: "kb-cancel";
        delay: 0;
    }

    filebrowser {
        directories-first: true;
        sorting-method: "name";
    }
}
